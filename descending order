package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		ArrayList<Integer>hm=new ArrayList<>();
		Scanner sc=new Scanner(System.in);
			int n=sc.nextInt();
			int [] array1=new int[n];
			for(int i=0;i<n;i++) {
				array1[i]=sc.nextInt();
				
			}
			Arrays.sort(array1);
			for(int j=n-1;j>=0;j--) {
				hm.add(array1[j]);
			}
			for(int gh:hm) {
				System.out.print(gh+" ");
			}
		}
	
	
}
