package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		ArrayList<String> hm=new ArrayList<>();
		Scanner sc=new Scanner(System.in);
		int number=sc.nextInt();
		String sathwik=Integer.toString(number);
		int Balu_len=sathwik.length();
		for(int i=Balu_len-1;i>=0;i--) {
			hm.add(String.valueOf(sathwik.charAt(i)));
			
		}
		System.out.println(String.join(" ",hm));
	}
}


