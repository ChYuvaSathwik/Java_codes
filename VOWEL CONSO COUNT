package com.sathwik.first;
import java.util.*;
public class FirstProgram{
	public static void main(String [] args) {
		String vowel="aeiouAEIOU";
		String name="sathwikSATHWIK";
		int count1=0;
		int count2=0;
		for(char i:name.toCharArray()) {
			if (vowel.contains(i+"")) {
				count1++;
				
			}
			else {
				count2++;
				
			}
			
		}
		System.out.println(count1);
		System.out.println(count2);
	}
	}
"""HI BRO CONVERTING TO PYTHON TO JAVA"""
